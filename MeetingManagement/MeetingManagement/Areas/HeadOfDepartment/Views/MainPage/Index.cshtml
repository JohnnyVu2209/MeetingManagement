@model IEnumerable<MeetingManagement.Models.MEETING>

@{
    ViewBag.Title = "Index";
}
<link href="~/Content/catalog-list.css" rel="stylesheet" />
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<br />
<ul class="nav nav-tabs">
    <li class="active"><a data-toggle="tab" href="#joinedMeetings">Cuộc họp tham dự</a></li>
    <li><a data-toggle="tab" href="#myMeetings">Cuộc họp của tôi</a></li>
</ul>

<div class="tab-content">
    <div id="joinedMeetings" class="tab-pane fade in active">
        <br />
        <ul class="catalogList">
            @foreach (var item in Model)
            {
                <li>
                    @item.Meeting_name
                </li>
            }
        </ul>
    </div>
    <div id="myMeetings" class="tab-pane fade">
        <br />
        <ul class="catalogList">
            @foreach (var item in Model)
            {
                <li>
                    @item.Meeting_id
                </li>
            }
        </ul>
    </div>
</div>
<p>
    @ViewBag.userID
</p>