@model IEnumerable<MeetingManagement.Models.MEETING>

<link href="~/Content/catalog-list.css" rel="stylesheet" />
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

<ul class="catalogList">
    @foreach (var item in Model)
    {
        if (item.Create_by.ToString() == ViewBag.user_identity)
        {
            <li style="display: flex; align-items: center;" onclick="location.href = '@Url.Action("MeetingDetail","MEETINGs", new { id = item.Meeting_id, modify = false })';">
                @item.Meeting_name
                <i style="position:absolute; left:0px;" class="fas fa-edit"></i>
            </li>
        }
        else
        {
            <li onclick="location.href = '@Url.Action("MeetingDetail","MEETINGs", new { id = item.Meeting_id, modify = false })';">
                @item.Meeting_name
            </li>
        }
    }
</ul>
<style>
    .catalogList > li:hover {
        transform: none !important;
    }

    .catalogList > li {
        padding: 0 25px;
    }
</style>
